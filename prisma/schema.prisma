generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Session {
    id        String   @id
    sid       String   @unique
    data      String
    expiresAt DateTime
}

model User {
    id       String @id @default(uuid())
    username String @unique
    password String
    role     Role   @default(BASIC)
}

enum Role {
    BASIC
    ADMIN
}
